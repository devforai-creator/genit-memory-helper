{
  "name": "genit-memory-helper",
  "version": "1.3.5",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "sync:version": "node scripts/sync-version.js",
    "pretest": "npm run build",
    "test": "vitest --run tests/unit",
    "test:unit": "vitest --run",
    "test:smoke": "playwright test",
    "fingerprint": "node scripts/fingerprint.js",
    "version": "npm run sync:version && npm run build && git add genit-memory-helper.user.js package.json package-lock.json",
    "bump:patch": "npm version patch && git push --follow-tags",
    "bump:minor": "npm version minor && git push --follow-tags",
    "bump:major": "npm version major && git push --follow-tags"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@types/node": "^20.11.30",
    "jsdom": "^24.0.0",
    "playwright": "^1.47.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  }
}
